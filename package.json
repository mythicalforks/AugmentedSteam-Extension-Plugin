{
  "scripts": {
    "dev": "millennium-ttc --build dev",
    "watch": "nodemon --watch webkit --watch frontend --ext ts,tsx,js,jsx --exec \"millennium-ttc --build dev\"",
    "update-augmented-steam": "git submodule update --remote AugmentedSteam",
    "build-augmented-steam": "cd AugmentedSteam && npm install && npm run build -- -- chrome --production",
    "build": "npm run build-augmented-steam && millennium-ttc --build prod"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^11.0.1",
    "@types/react": "16.14.0",
    "@types/react-dom": "^18.3.2",
    "@types/webpack": "^5.28.5",
    "nodemon": "^3.1.7",
    "semantic-release": "^24.2.0"
  },
  "type": "module",
  "name": "augmented-steam-plugin",
  "version": "1.0.3",
  "description": "Augmented Steam plugin for Millennium",
  "main": "./frontend/index.jsx",
  "author": "BossSloth",
  "license": "MIT",
  "dependencies": {
    "@steambrew/api": "^4.2.0",
    "@steambrew/client": "^4.2.0",
    "@steambrew/ttc": "^1.0.5",
    "@steambrew/webkit": "^4.2.0",
    "augmented-steam": "./AugmentedSteam"
  }
}
